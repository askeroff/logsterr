{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "jsx-a11y/href-no-hash": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": ["error", "windows"],
    "class-methods-use-this": "off",
    "react/forbid-prop-types": "off",
    "jsx-a11y/label-has-for": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
